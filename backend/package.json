{
  "name": "@vidverse/backend",
  "version": "0.1.0",
  "description": "VidVerse Backend API Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "db:migrate": "tsx scripts/run-migration.ts",
    "db:generate": "node scripts/generate-types.js",
    "fetch-models": "tsx scripts/fetch-replicate-models.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.932.0",
    "@aws-sdk/client-s3": "^3.932.0",
    "@aws-sdk/s3-request-presigner": "^3.932.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@sentry/node": "^7.91.0",
    "@types/multer": "^2.0.0",
    "aws-jwt-verify": "^5.1.1",
    "bullmq": "^5.63.2",
    "dotenv": "^16.4.1",
    "fastify": "^4.25.2",
    "fluent-ffmpeg": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "multer": "^2.0.2",
    "openid-client": "^5.7.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "replicate": "^1.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "pino-pretty": "^13.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
