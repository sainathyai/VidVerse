{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadGetObject",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::vidverse-assets/*"
    },
    {
      "Sid": "AllowAppUploads",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::971422717446:root"
      },
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::vidverse-assets/*"
    }
  ]
}


